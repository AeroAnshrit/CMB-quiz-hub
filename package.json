{
  "name": "engineeringquiz",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://aeroanshrit.github.io/CMB-quiz-hub",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "validate": "node scripts/validate_data.js",
    "build": "npx tailwindcss -i ./public/input.css -o ./public/css/styles.css",
    "watch": "npx tailwindcss -i ./public/input.css -o ./public/css/styles.css --watch",
    "predeploy": "node -e \"require('fs-extra').copy('data', 'docs/data', { overwrite: true }, err => { if (err) return console.error(err); console.log('Data copied to docs folder for deployment.'); });\"",
    "deploy": "npm run predeploy && gh-pages -d docs"
  },
