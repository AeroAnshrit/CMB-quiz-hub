    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "glob": "^10.4.5",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "jest": "^29.0.0",
    "tailwindcss": "^3.4.3"
  }
}
